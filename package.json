{
  "name": "eleventy-duo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^0.6.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.2.2",
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^8.4.1",
    "css-loader": "^6.11.0",
    "cssnano": "^4.1.11",
    "eleventy-plugin-reading-time": "^0.0.1",
    "html-minifier": "^4.0.0",
    "node-gyp": "^10.0.1",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.5.6",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^4.3.0",
    "postcss-preset-env": "^6.7.2",
    "terser-webpack-plugin": "^5.3.16",
    "webpack": "^5.104.1",
    "webpack-cli": "^4.10.0",
    "webpack-manifest-plugin": "^4.1.1"
  },
  "scripts": {
    "dev": "bun run dev:assets & bun run dev:site",
    "start": "bunx eleventy --serve",
    "build": "bun install && bun run clean && bun run build:assets && bun run build:site",
    "dev:assets": "env ELEVENTY_ENV=development bunx webpack --mode production --watch",
    "dev:site": "env ELEVENTY_ENV=development bunx eleventy --serve",
    "build:assets": "bunx webpack --mode production",
    "build:site": "env ELEVENTY_ENV=production bunx eleventy",
    "clean": "rm -rf ./public"
  },
  "dependencies": {
    "remove": "^0.1.5"
  },
  "packageManager": "bun@1.3.1"
}